{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "run current python file",
            "type": "shell",
            "command": "poetry",
            "args": [
                "run",
                "python",
                "-m",
                "object_detection.faster_rcnn.faster_rcnn"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },            
            "group": {
                "kind": "build",
                "isDefault": true
            },
        }
    ]
}